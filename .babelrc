{
	"presets": [
		"@babel/env",
		"@babel/react",
		[
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"transform-html-import-to-string",
		"@babel/plugin-transform-react-jsx",
		[
			"babel-plugin-styled-components",
			{
				"ssr": false,
				"displayName": true,
				"preprocess": false
			}
		]
	]
}